<%- include("_header") -%>

    <h1>Edit Customer</h1>
    <form action="/edit" method="POST">
            <table>
                <tr>
                    <td><label for="cusid">ID</label>:</td>
                    <td><input type="number" name="cusid" id="cusid" value="<%= customer.cusid %>" /></td>
                </tr>
                <tr>
                    <td><label for="cusFname">First Name</label>:</td>
                    <td><input type="text" name="cusfname" id="cusfname" value="<%= customer.cusfname %>" /></td>
                </tr>
                <tr>
                    <td><label for="cusLname">Last Name</label>:</td>
                    <td><input type="text" name="cuslname" id="cuslname" value="<%= customer.cuslname %>" /></td>
                </tr>
                <tr>
                    <td><label for="cusstate">State</label>:</td>
                    <td><input type="text" name="cusstate" id="cusstate" value="<%= customer.cusstate %>" /></td>
                </tr>
                <tr>
                    <td><label for="cusSalesytd">State YTD</label>:</td>
                    <td><input type="text" name="cussalesytd" id="cusSalesytd" value="<%= customer.cussalesytd %>" /></td>
                </tr>
                <tr>
                    <td><label for="cussalesprev">Previous Years Sales</label>:</td>
                    <td><input type="text" name="cussalesprev" id="cussalesprev" value="<%= customer.cussalesprev %>" /></td>
                </tr>
        <tr>
            <td>
                <input type="submit" value="Submit" />

            </td>
        </tr>
            </table>
    </form>
    <%- include("_footer") -%>